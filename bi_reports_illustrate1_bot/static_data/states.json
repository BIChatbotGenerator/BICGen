    "keyboards": ["back_and_home"],
    "msgs": ["Just one step remained\\!\nWhat is your intended chart 📊\\?"],
    "jump": "query_filter_run"
  },
  "auth_home_reportsList": {
    "keyboards": ["back_and_home", "reportsList"],
    "msgs": ["Welcome to archive of your reports\\!"]
  }
}
